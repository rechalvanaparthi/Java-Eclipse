package collections;


import java.util.ArrayList;

/*
 Array List
 --------
 Maintain Insertion
 Duplicates are allowed
 */
public class ArrayListDemo2 {
	
	public static void main(String[] args) {
	ArrayList a1 = new ArrayList();
	System.out.println(a1.size());
	
	//adding
	a1.add(100);
	a1.add(200);
	a1.add("Java");
	a1.add("Python");
	a1.add(45000.0);
	a1.add(35000.0);
	
	System.out.println(a1.size());
	System.out.println(a1);
	}
}