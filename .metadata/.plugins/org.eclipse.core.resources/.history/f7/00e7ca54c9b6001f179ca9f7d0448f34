package sirproject;

public class EmployeeTest {
	public static void main(String[] args) {
		
		Employee e2 = new Employee(1,"Wills",55000,"wills@gmail.com");
		System.out.println(e2);
		
		
		Employee e3 = new Employee();
		e2.setEmployeeId(101);
		e2.setEmployeeName("Jones");
		e2.setEmployeeSalary(55000);
		e2.setEmployeeEmail("jones@gmail.com");
		
		System.out.println(e3.getEmployeeId());
		System.out.println(e3.getEmployeeName());
		System.out.println(e3.getEmployeeSalary());
		System.out.println(e3.getEmployeeEmail());
	}
}
