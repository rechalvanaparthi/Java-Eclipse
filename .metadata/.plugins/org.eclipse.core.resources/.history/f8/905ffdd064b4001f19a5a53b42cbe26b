package java8features;

@FunctionalInterface
interface MyFunctionalInterface{
	void greet(String name,int id);
}
public class LambaExpressions {
	public static void main(String[] args) {
		MyFunctionalInterface myfun = (String name,int id) -> System.out.println("Hello...."+name +id);
		myfun.greet("Raaj",1001);
	}

}
