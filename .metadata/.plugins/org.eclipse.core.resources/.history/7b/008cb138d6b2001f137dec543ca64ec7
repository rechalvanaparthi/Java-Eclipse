package multithreading;

class MulTable{
	public void printMulTable(int num) {
		for(int i=1;i<=10;i++) {
			//10*1=10
			System.out.println(num+"*"+i+"="+(num*i));
			try {
				Thread.sleep(1000);
			}catch(InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}
public class SynchronizationDemo {
	public static void main(String[] args) {
		
	}
}
